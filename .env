COMPOSE_PROJECT_NAME=microservice-structure
# SQLALCHEMY_SQLITE_DATABASE_MACHINE_URI=sqlite+aiosqlite:////database/machine.db
SQLALCHEMY_SQLITE_DATABASE_URI=sqlite+aiosqlite:////database/database.db
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5671
RABBITMQ_USER=guest
RABBITMQ_PASSWD=guest
RABBITMQ_USE_TLS=1
