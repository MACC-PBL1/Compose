# RABBITMQ_PORT=5671
# RABBITMQ_USE_TLS=1
RABBITMQ_CERT_PATH=/etc/rabbitmq/ssl
RABBITMQ_CA_CERT_PATH=/etc/rabbitmq/ssl/ca_cert.pem
RABBITMQ_CLIENT_CERT_PATH=/etc/rabbitmq/ssl/client_cert.pem
RABBITMQ_CLIENT_KEY_PATH=/etc/rabbitmq/ssl/client_key.pem
RABBITMQ_PREFETCH_COUNT=10
