          "query": "active = from(bucket: \"events\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r._measurement == \"haproxy\")\n  |> filter(fn: (r) => r._field == \"active_servers\")\n  |> filter(fn: (r) => r.type == \"backend\")\n  |> last()\n  |> group()\n  |> sum()\n  |> findRecord(fn: (key) => true, idx: 0)\n\nbackup = from(bucket: \"events\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r._measurement == \"haproxy\")\n  |> filter(fn: (r) => r._field == \"backup_servers\")\n  |> filter(fn: (r) => r.type == \"backend\")\n  |> last()\n  |> group()\n  |> sum()\n  |> findRecord(fn: (key) => true, idx: 0)\n\nfrom(bucket: \"events\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r._measurement == \"haproxy\")\n  |> limit(n: 1)\n  |> map(fn: (r) => ({_value: if (active._value + backup._value) > 0 then float(v: active._value) / float(v: active._value + backup._value) * 100.0 else 0.0, _time: r._time}))",
