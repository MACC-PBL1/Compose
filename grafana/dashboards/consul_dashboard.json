          "query": "passing = from(bucket: \"events\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r._measurement == \"consul_health_checks\")\n  |> filter(fn: (r) => r._field == \"passing\")\n  |> last()\n  |> group()\n  |> sum()\n  |> findRecord(fn: (key) => true, idx: 0)\n\ntotal = from(bucket: \"events\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r._measurement == \"consul_health_checks\")\n  |> filter(fn: (r) => r._field == \"passing\" or r._field == \"warning\" or r._field == \"critical\")\n  |> last()\n  |> group()\n  |> sum()\n  |> findRecord(fn: (key) => true, idx: 0)\n\nfrom(bucket: \"events\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r._measurement == \"consul_health_checks\")\n  |> limit(n: 1)\n  |> map(fn: (r) => ({_value: if total._value > 0 then float(v: passing._value) / float(v: total._value) * 100.0 else 0.0, _time: r._time}))",
          "refId": "A"
        }
      ],
